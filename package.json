{
  "name": "api-node-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node ./build/index.js",
    "lint": "eslint 'src/**/*.ts' --fix",
    "test": "jest",
    "knex:migrate": "npx knex --knexfile ./src/server/database/knex/Envirroment.ts migrate:latest",
    "knex:rollback": "npx knex --knexfile ./src/server/database/knex/Envirroment.ts migrate:rollback",
    "knex:rollbackall": "npx knex --knexfile ./src/server/database/knex/Envirroment.ts migrate:rollback --all",
    "knex:seed": "npx knex --knexfile ./src/server/database/knex/Envirroment.ts seed:run"
  },
  "keywords": [],
  "author": "Welliton Galant",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.2",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.0",
    "express": "^4.21.2",
    "http-status-codes": "^2.3.0",
    "jest": "^29.7.0",
    "knex": "^3.1.0",
    "path-to-regexp": "^8.2.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "undici": "^7.4.0",
    "vercel": "^41.3.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/express": "^5.0.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.21.0",
    "eslint-plugin-node": "^11.1.0",
    "globals": "^16.0.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  }
